{
  "name": "n8n-nodes-data-diff-clickzetta",
  "version": "1.0.0",
  "description": "N8N custom nodes for data comparison using data-diff with Clickzetta support",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "data-diff",
    "clickzetta",
    "data-comparison",
    "database"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yunqiqiliang/data-diff-n8n",
  "author": {
    "name": "Data-Diff N8N Team",
    "email": "support@data-diff-n8n.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yunqiqiliang/data-diff-n8n.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run copy-resources && npm run copy-package",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "copy-resources": "cp -r src/resources dist/",
    "copy-package": "cp package.dist.json dist/package.json",
    "test": "jest",
    "lint": "eslint . --ext .ts --fix",
    "dev": "nodemon --exec npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ClickzettaApi.credentials.js",
      "dist/credentials/DataDiffConfig.credentials.js",
      "dist/credentials/DatabaseConnectorCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ClickzettaConnector/ClickzettaConnector.node.js",
      "dist/nodes/DatabaseConnector/DatabaseConnector.node.js",
      "dist/nodes/DataComparison/DataComparison.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0",
    "n8n-core": "^1.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
